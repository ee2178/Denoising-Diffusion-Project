{
    "model": {
        "C": 1,
        "K": 30,
        "M": 169,
        "P": 7,
        "adaptive": true,
        "s": 2
    },
    "mri": {
        "R": 8,
        "acs_lines": 24
    },
    "paths": {
        "ckpt": "trained_nets/LPDSNet/net.ckpt",
        "save": "trained_nets/LPDSNet"
    },
    "train": {
        "fit": {
            "backtrack_thresh": 2,
            "clip_grad": 0.05,
            "demosaic": false,
            "epochs": 6000,
            "mcsure": false,
            "noise_std": [
                10,
                13
            ],
            "save_freq": 5,
            "val_freq": 50,
            "verbose": true
        },
        "loaders": {
            "batch_size": 1,
            "crop_size": 256,
            "scaling_fac": 2000.0,
            "trn_kspace_path_list": [
                "../../datasets/fastmri/brain/multicoil_train"
            ],
            "trn_path_list": [
                "../../datasets/fastmri_preprocessed/brain_T2W_coil_combined/train"
            ],
            "tst_kspace_path_list": [
                "../../datasets/fastmri/brain/multicoil_test"
            ],
            "tst_path_list": [
                "../../datasets/fastmri_preprocessed/brain_T2W_coil_combined/test"
            ],
            "val_kspace_path_list": [
                "../../datasets/fastmri/brain/multicoil_val"
            ],
            "val_path_list": [
                "../../datasets/fastmri_preprocessed/brain_T2W_coil_combined/val"
            ]
        },
        "opt": {
            "lr": 0.001
        },
        "sched": {
            "gamma": 0.95,
            "step_size": 50
        }
    },
    "type": "LPDSNet"
}